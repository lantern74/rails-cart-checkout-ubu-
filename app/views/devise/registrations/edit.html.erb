			<!-- Table START -->
			<div class="table-responsive border-0">
				<table class="table table-dark-gray align-middle p-4 mb-0 table-hover">

					<!-- Table head -->
					<thead>
						<tr>
							<th scope="col" class="border-0 rounded-start">Domain</th>
							<th scope="col" class="border-0">IP</th>
							<th scope="col" class="border-0">Time added</th>
							<th scope="col" class="border-0 rounded-end">Status</th>
							<th scope="col" class="border-0 rounded-end">Action</th>
						</tr>
					</thead>

					<!-- Table body START -->
					    <tbody>
        <!-- Iterate over domain records -->
        <% current_user.domains.each do |domain| %>
        <!-- Table row -->
        <tr id="domain-<%= domain.id %>">
						<td><%= domain.name %></td>
						<td> <%#= domain.ip %></td>
						<td><%= domain.created_at.strftime("%d %b %Y") %></td>
						<td>
								<span class="verification-status badge <% if domain.verified? %>text-bg-success<% else %>bg-light border-1 rounded-3 text-dark<% end %>" style="padding:8px; font-size:14px">
										<% if domain.verified? %>
												Verified
										<% else %>
												Not Verified
										<% end %>
								</span>
								<% unless domain.verified? %>
										<%= link_to "Verify", verify_domain_path(domain), class: "btn btn-sm btn-success text-white me-1 mb-1 mb-md-0" %>


								<% end %>

  <% if flash[:alert] %>
								<br>
								<br>
    <div class="mb-3 text-danger" style="font-size:14px;">
      <%= raw flash[:alert] %>
    </div>
  <% end %>

						</td>
						<td>
								<%= button_to "Remove", domain_path(domain), method: :delete, data: { confirm: "Are you sure you want to delete this domain?" }, class: "btn btn-sm btn-danger-soft me-1 mb-1 mb-md-0" %>
						</td>
				</tr>
        <% end %>
    </tbody>
					<!-- Table body END -->
				</table>
			</div>
			<!-- Table END -->
		</div>
		<!-- Card body END -->
	</div>





